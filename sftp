Activating Module Federation for Angular Projects
ng generate app shell --style scss --ssr false
ng generate app mfe --style scss --ssr=false
ng add @angular-architects/module-federation --project shell --port 4200 --type host
ng add @angular-architects/module-federation --project mfe --port 4201 --type remote
ng generate environments --project=shell
ng generate environments --project=mfe



packagejson:
npm i concurrently
kill port installieren
"start": "npm run start:shell",
"start:shell": "ng serve shell  -o --port 4200",
"start:mfe": "ng serve mfe -o --port 4201",
"start:all": "concurrently \"npm run start:shell\" \"npm run start:mfe\" ",
"build:shell": "ng build shell --prod",
"build:mfe": "ng build mfe --prod",
"build:core": "ng build core --prod",

